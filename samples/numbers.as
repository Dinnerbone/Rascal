// Decimal integers
var i0 = 0;
var i1 = 1;
var i42 = 42;
var iLarge = 2147483647; // max i32
var iLargePlusOne = 2147483648; // one over i32

// Floating-point (decimal)
var f0 = 0.0;
var f1 = 1.23;
var fLeadingDot = .5;  // leading dot
var fTrailingDot = 5.; // trailing dot

trace("i0 = " + i0);
